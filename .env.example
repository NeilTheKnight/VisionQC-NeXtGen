# VisionQC NeXtGen Environment Configuration
# Copy this file to .env.development, .env.staging, or .env.production
# and update the values according to your environment

# Application Configuration
VITE_APP_TITLE=VisionQC NeXtGen
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api
VITE_WS_URL=ws://localhost:3001/ws

# Authentication Configuration
VITE_AUTH_TOKEN_KEY=visionqc_token
VITE_AUTH_USER_KEY=visionqc_user

# Feature Flags
VITE_ENABLE_MOCK_DATA=true
VITE_ENABLE_REAL_TIME_UPDATES=true
VITE_ENABLE_CAMERA_SIMULATION=true

# Debug Configuration
VITE_DEBUG=true
VITE_LOG_LEVEL=info

# External Services (Optional)
# VITE_SENTRY_DSN=your_sentry_dsn_here
# VITE_ANALYTICS_ID=your_analytics_id_here

# Camera Configuration
VITE_DEFAULT_CAMERA_FPS=15
VITE_MAX_DETECTION_CONFIDENCE=100
VITE_MIN_DETECTION_CONFIDENCE=50

# Performance Configuration
VITE_STATS_UPDATE_INTERVAL=3000
VITE_ALERT_DISPLAY_DURATION=5000
VITE_RECONNECT_INTERVAL=5000
